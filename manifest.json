{
  "manifest_version": 3,
  "name": "DSA Organizer",
  "version": "1.0",
  "description": "Organize DSA questions by topics.",
  "permissions": ["storage", "tabs"],

  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },

  "background": {
    "service_worker": "background.js"
  }
}
